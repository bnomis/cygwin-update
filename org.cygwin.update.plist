<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key><string>org.cygwin.update</string>
	<key>WorkingDirectory</key><string>/var/root/src/cygwin-update</string>
    <key>Program</key><string>/var/root/src/cygwin-update/cygwin.py</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>2</integer>
        <key>Minute</key>
        <integer>10</integer>
    </dict>
    <key>StandardOutPath</key><string>/var/root/logs/cygwin-stdout.log</string>
    <key>StandardErrorPath</key><string>/var/root/logs/cygwin-stderr.log</string>
</dict>
</plist>
